---
import "../styles/global.css";
import { AstroSeo } from "@astrolib/seo";
const url = "https://ceo.dossware.com";
---

<AstroSeo
  title="DOSE - Paradorn Lungchoy, CEO of Dossware"
  canonical={url}
  description="Paradorn Lungchoy, CEO of Dossware Co., Ltd., is a software engineer, entrepreneur, and educator."
  openGraph={{
    url: url,
    title: "DOSE - Paradorn Lungchoy, CEO of Dossware",
    description: "Paradorn Lungchoy, CEO of Dossware Co., Ltd., is a software engineer, entrepreneur, and educator.",
    images: [
      {
        url: "/D.png",
        width: 1200,
        height: 630,
        alt: "Paradorn Lungchoy - CEO of Dossware",
        type: "image/png",
      },
    ],
    site_name: "Dossware",
    type: "website",
    locale: "en_US",
  }}
  twitter={{
    handle: "@yourhandle",
    site: "@yoursite",
    cardType: "summary_large_image",
  }}
/>
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />

<!-- Basic Meta Tags -->
<title>DOSE - Paradorn Lungchoy, CEO of Dossware</title>
<meta
  name="description"
  content="Paradorn Lungchoy, CEO of Dossware Co., Ltd., is a software engineer, entrepreneur, and educator."
/>
<meta name="keywords" content="Paradorn Lungchoy, software engineer, entrepreneur, educator" />
<meta name="author" content="Paradorn Lungchoy" />

<!-- Favicons -->
<link rel="icon" href="/D.png" sizes="any" />
<link rel="icon" href="/icon.svg" type="image/svg+xml" />
<link rel="apple-touch-icon" href="/D.png" />
<link rel="manifest" href="/manifest.webmanifest" />

<!-- Favicon for IE -->
<link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />

<!-- Open Graph / Facebook -->
<meta property="og:type" content="website" />
<meta property="og:url" content="https://ceo.dossware.com" />
<meta property="og:title" content="DOSE - Paradorn Lungchoy, CEO of Dossware" />
<meta
  property="og:description"
  content="Paradorn Lungchoy, CEO of Dossware Co., Ltd., is a software engineer, entrepreneur, and educator."
/>
<meta property="og:image" content="/D.png" />

<!-- Twitter -->
<meta property="twitter:card" content="summary_large_image" />
<meta property="twitter:url" content="https://ceo.dossware.com" />
<meta property="twitter:title" content="DOSE - Paradorn Lungchoy, CEO of Dossware" />
<meta
  property="twitter:description"
  content="Paradorn Lungchoy, CEO of Dossware Co., Ltd., is a software engineer, entrepreneur, and educator."
/>
<meta property="twitter:image" content="/D.png" />

<!-- Canonical URL -->
<link rel="canonical" href="https://ceo.dossware.com" />

<!-- Additional SEO -->
<meta name="robots" content="index, follow" />
<meta name="googlebot" content="index, follow" />

<!-- Apple Touch Icon (already included in favicons, but keeping for backwards compatibility) -->
<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png" />

<!-- Theme Color for Mobile Browsers -->
<meta name="theme-color" content="#ffffff" />

<!-- For IE -->
<meta http-equiv="X-UA-Compatible" content="IE=edge" />

<!--- Typography -->
<!--- 
Instrument Serif: Google Fonts
Inter: Rasmus CDN
-->

<link rel="preconnect" href="https://fonts.googleapis.com" />
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
<link
  href="https://fonts.googleapis.com/css2?family=Instrument+Serif:ital@0;1&display=swap"
  rel="stylesheet"
/>
<link rel="preconnect" href="https://rsms.me/" />
<link rel="stylesheet" href="https://rsms.me/inter/inter.css" />
