---
import Layout from "../layouts/Layout.astro";
import Search from '../components/Search.jsx'
---

<Layout title="chrome">
    <div>
        <section class="py-14">
            <div class="max-w-screen-xl mx-auto px-4 text-center md:px-8">
                <div class="max-w-xl mx-auto">
                    <h3 class="text-green-700 text-6xl font-bold sm:text-4xl">
                        HELLO DOSE
                    </h3>
                    <Search client:load/>
                </div>
                <div class="flex mt-7 justify-center">
                    <div class="mr-6">
                        <h3 class="text-left font-bold text-green-500">
                            Social
                        </h3>
                        <ul class="flex flex-col items-start">
                            <li class="underline">
                                <a
                                    href="https://www.youtube.com/"
                                    target="_blank"
                                    rel="noopener noreferrer">YouTube</a>
                            </li>
                            <li class="underline">
                                <a
                                    href="https://twitter.com/"
                                    target="_blank"
                                    rel="noopener noreferrer">X (Twitter)</a>
                            </li>
                            <li class="underline">
                                <a
                                    href="https://github.com/"
                                    target="_blank"
                                    rel="noopener noreferrer">GitHub</a>
                            </li>
